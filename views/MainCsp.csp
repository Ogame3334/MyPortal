<html>
    <head>
        <title>OgamePortalSite</title>
        <link href="./myportal/style.css" rel="stylesheet">
        <link rel="icon" href="favicon.ico">
        <link rel="icon" type="image/png" href="./myportal/images/icon.webp">
    </head>
    <body>
        <header>
            <div class="header-pin-right"></div>
            <div class="header-pin-left"></div>
            <h1 class="headline">
                <a>PortalBoard</a>
            </h1>
        </header>
        <main>
            <!-- <div class="searchbox-pin-right"></div>
            <div class="searchbox-pin-left"></div> -->
            <div class="search-box">
                <a href="javascript:search();">
                    <img src="./myportal/images/search_button.webp" class="search-button">
                </a>
                <input class="search-text-box" type="text" id="search-text-box" onkeypress="down_enter(event.keyCode);" >
                <div class="suggest-box"></div>
            </div>
            <div class="cork-board">
                <br>
                <div class="board">
                    <div class="line-right"></div>
                    <div class="line-left"></div>
                    <div class="pin"></div>
                    <br>
                    <div class="panel">
                        <h2 class="category-name">Google</h2>
                        <div class="images">
                            <a href="https://www.google.com/">
                                <img class="c-btn" src="./myportal/images/Google/Google.webp">
                            </a>
                            <a href="https://www.youtube.com/">
                                <img class="c-btn" src="./myportal/images/Google/youtube.png">
                            </a>
                            <a href="https://mail.google.com/mail/">
                                <img src="./myportal/images/Google/gmail.png">
                            </a>
                            <a href="https://drive.google.com/drive/">
                                <img src="./myportal/images/Google/GoogleDrive.png">
                            </a>
                            <a href="https://www.google.co.jp/maps/">
                                <img src="./myportal/images/Google/GoogleMap.webp">
                            </a>
                        </div>
                    </div>
                </div>
                <div class="board">
                    <div class="line-right"></div>
                    <div class="line-left"></div>
                    <div class="pin"></div>
                    <br>
                    <div class="panel">
                        <h1 class="category-name">Subscription & Novel</h1>
                        <div class="images" style="display: flex; flex-wrap: wrap;" >
                            <a href="https://animestore.docomo.ne.jp/animestore/">
                                <img src="./myportal/images/Subscription/danime.webp">
                            </a>
                            <a href="https://www.amazon.co.jp/gp/video/storefront/">
                                <img src="./myportal/images/Subscription/primevideo.webp">
                            </a>
                            <a href="https://syosetu.com/">
                                <img src="./myportal/images/Novel/narou.webp">
                            </a>
                            <a href="https://kakuyomu.jp/">
                                <img src="./myportal/images/Novel/kakuyomu.webp">
                            </a>
                        </div>
                    </div>
                </div>
                <div class="board">
                    <div class="line-right"></div>
                    <div class="line-left"></div>
                    <div class="pin"></div>
                    <br>
                    <div class="panel">
                        <h1 class="category-name">Campas</h1>
                        <div class="images" style="display: flex; flex-wrap: wrap;" >
                            <a href="https://ict-i.el.kyutech.ac.jp/my/">
                                <img src="./myportal/images/Campas/moodle.png">
                            </a>
                            <a href="https://virginia.jimu.kyutech.ac.jp/">
                                <img src="./myportal/images/Campas/LiveCampas.png">
                            </a>
                            <a href="https://www.iizuka.kyutech.ac.jp/campuslife/schedule/">
                                <img src="./myportal/images/Campas/CampasCalender.png">
                            </a>
                            <a href="https://www.iizuka.kyutech.ac.jp/faculty/educational-info#i-4">
                                <img src="./myportal/images/Campas/schedule.png">
                            </a>
                            <a href="https://edragon-syllabus.jimu.kyutech.ac.jp/guest/syllabuses">
                                <img src="./myportal/images/Campas/syllabus.png">
                            </a>
                        </div>
                    </div>
                </div>
                <div class="board">
                    <div class="line-right"></div>
                    <div class="line-left"></div>
                    <div class="pin"></div>
                    <br>
                    <div class="panel">
                        <h1 class="category-name">SNS</h1>
                        <div class="images" style="display: flex; flex-wrap: wrap;" >
                            <a href="https://twitter.com/home/">
                                <img src="./myportal/images/SNS/twitter.webp">
                            </a>
                            <a href="https://mastodon.compositecomputer.club/">
                                <img src="./myportal/images/SNS/mastodon.webp">
                            </a>
                            <a href="https://www.instagram.com/">
                                <img src="./myportal/images/SNS/instagram.webp">
                            </a>
                            <a href="https://toybox.compositecomputer.club/">
                                <img src="./myportal/images/SNS/ToyBox.webp">
                            </a>
                        </div>
                    </div>
                </div>
                <div class="board">
                    <div class="line-right"></div>
                    <div class="line-left"></div>
                    <div class="pin"></div>
                    <br>
                    <div class="panel">
                        <h1 class="category-name">Learning</h1>
                        <div class="images" style="display: flex; flex-wrap: wrap;" >
                            <a href="https://chat.openai.com/">
                                <img src="./myportal/images/Lerning/chatgpt.png">
                            </a>
                            <a href="https://www.deepl.com/ja/translator">
                                <img src="./myportal/images/Lerning/deepl.webp">
                            </a>
                            <a href="https://www.desmos.com/calculator?lang=ja">
                                <img src="./myportal/images/Lerning/desmos.webp">
                            </a>
                            <a href="https://ja.overleaf.com/project">
                                <img src="./myportal/images/Lerning/overleaf.png">
                            </a>
                        </div>
                    </div>
                </div>
                <div class="board">
                    <div class="line-right"></div>
                    <div class="line-left"></div>
                    <div class="pin"></div>
                    <br>
                    <div class="panel">
                        <h1 class="category-name">Other</h1>
                        <div class="images" style="display: flex; flex-wrap: wrap;" >
                            <a href="https://github.com/Ogame3334/">
                                <img src="./myportal/images/Other/github.png">
                            </a>
                            <a href="https://ogame3334.github.io/MyPage/?anim=false">
                                <img src="./myportal/images/Other/icon.webp">
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </main>
        <script src="./myportal/script.js"></script>
    </body>
</html>